{
  "name": "redux-persist-transform-react-redux-fetch",
  "version": "1.0.0",
  "description": "react-redux-fetch transform for redux-persist",
  "main": "lib/index.js",
  "typings": "index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run clean && npm run build",
    "prettify": "prettier --write \"src/**/*.js*\""
  },
  "repository": "https://github.com/hirviid/redux-persist-transform-react-redux-fetch",
  "author": "hirviid",
  "license": "MIT",
  "peerDependencies": {
    "redux-persist": "^4.0.0",
    "seamless-immutable": "^7.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@types/redux-persist": "^4.3.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "rimraf": "^2.6.3",
    "seamless-immutable": "^7.1.4"
  },
  "lint-staged": {
    "src/**/*.ts*": [
      "prettier --trailing-comma es5 --single-quote --print-width 100 --write",
      "git add"
    ]
  },
  "files": [
    "lib",
    "src",
    "index.d.ts"
  ],
  "keywords": [
    "redux",
    "redux-persist",
    "redux-persist-transform",
    "react-redux-fetch"
  ]
}
